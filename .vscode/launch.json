    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Prep champ data",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/data",
            "program": "${workspaceFolder}/data/prep_champ.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Prep champ wiki data",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/data",
            "program": "${workspaceFolder}/data/prep_champ_wiki.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Prep item data",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "cwd": "${workspaceFolder}/data",
            "program": "${workspaceFolder}/data/prep_item.js"
        }
    ]
}